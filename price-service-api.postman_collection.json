    {
      "name": "Auth",
      "item": [
        {
          "name": "Login (get JWT)",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "pm.test('Status is 200', function () { pm.response.to.have.status(200); });",
                  "let jsonData = {};",
                  "try { jsonData = pm.response.json(); } catch (e) { jsonData = {}; }",
                  "pm.test('Response has token', function () { pm.expect(jsonData.token).to.exist; });",
                  "if (jsonData.token) {",
                  "  pm.environment.set('jwt', jsonData.token);",
                  "  if (jsonData.expiresIn) pm.environment.set('jwt_expiresIn', jsonData.expiresIn.toString());",
                  "  if (jsonData.tokenType) pm.environment.set('jwt_type', jsonData.tokenType);",
                  "}"
                ],
                "type": "text/javascript",
                "packages": {}
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"miguel@inditex.com\",\n  \"password\": \"password\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/v1/auth/login",
              "host": [
                "{{baseUrl}}"
              ],
              "path": [
                "api",
                "v1",
                "auth",
                "login"
              ]
            },
            "description": "Authenticate and receive a JWT. Uses the email/password expected by your current AuthController."
          },
          "response": []
        }
      ]
    },
